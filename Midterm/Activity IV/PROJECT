import java.util.Scanner;

public class GESITE_ACTIVITY_VII {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter a non-negative integer: ");
        int num = sc.nextInt();     
        int 1 = 1; fact = 1;
        while(i <= num) {
            fact = fact * i;
            i++;
        }

        System.out.println(fact);
    }
}