class DIMCO_LAURENCE_IV {
    public static void main (String[] args) {
        int a = 10;
        int b = 8;
        int c = 2; 
     
        int sum = (a+b+c);
        int difference = (a-b-c);
        int product = (a*b*c);
        int qoutient = (a/b/c);
        
        System.out.println(sum);
        System.out.println(difference);
        System.out.println(product);
        System.out.println(qoutient);

        
    }
}